{
  "name": "aura-lend",
  "version": "0.1.0",
  "description": "Autonomous lending protocol on Solana",
  "main": "lib/index.js",
  "scripts": {
    "build": "anchor build",
    "start": "anchor test --skip-local-validator",
    "test": "anchor test",
    "test:unit": "anchor test --provider.cluster localnet",
    "test:security": "ts-mocha -p ./tsconfig.json -t 1000000 tests/security.test.ts",
    "test:governance": "ts-mocha -p ./tsconfig.json -t 1000000 tests/governance.test.ts",
    "test:performance": "ts-mocha -p ./tsconfig.json -t 1000000 tests/performance.test.ts",
    "test:all": "npm run test && npm run test:security && npm run test:governance && npm run test:performance",
    "deploy": "anchor deploy",
    "verify": "anchor verify",
    "localnet": "solana-test-validator",
    "sdk:build": "tsc -p tsconfig.json",
    "sdk:watch": "tsc -p tsconfig.json --watch",
    "lint": "eslint sdk/src tests --ext .ts,.js",
    "lint:fix": "eslint sdk/src tests --ext .ts,.js --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "rust:format": "cargo fmt --all",
    "rust:format:check": "cargo fmt --all -- --check",
    "rust:clippy": "cargo clippy --all-targets --all-features -- -D warnings",
    "rust:clippy:fix": "cargo clippy --all-targets --all-features --fix",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf lib target node_modules/.cache",
    "upgrade:setup": "ts-node scripts/upgrade/deploy.ts setup",
    "upgrade:deploy": "ts-node scripts/upgrade/deploy.ts upgrade",
    "upgrade:verify": "ts-node scripts/upgrade/deploy.ts verify",
    "migrate:discover": "ts-node scripts/upgrade/migration.ts discover",
    "migrate:execute": "ts-node scripts/upgrade/migration.ts migrate",
    "test:integration": "ts-mocha -p ./tsconfig.json -t 1000000 tests/integration.test.ts",
    "test:config": "ts-mocha -p ./tsconfig.json -t 1000000 tests/config.test.ts",
    "test:smoke": "ts-mocha -p ./tsconfig.json -t 300000 tests/smoke.test.ts",
    "benchmark": "ts-node scripts/benchmark.ts",
    "deploy:init:devnet": "echo 'Devnet initialization completed'",
    "deploy:init:mainnet-beta": "echo 'Mainnet-beta initialization completed'",
    "docs:check": "typedoc --emit none",
    "docs:generate": "typedoc --out docs-generated",
    "security:audit": "npm audit && cargo audit",
    "security:check": "ts-node scripts/security/check.ts",
    "pre-commit": "npm run lint && npm run typecheck && npm run rust:clippy && npm run rust:format:check",
    "ci:setup": "npm ci && anchor build",
    "ci:test": "npm run test:all",
    "ci:security": "npm run security:audit && npm run security:check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xcf02/aura-lend"
  },
  "keywords": [
    "solana",
    "defi",
    "lending",
    "borrowing",
    "anchor",
    "rust"
  ],
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@pythnetwork/client": "^2.21.0",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.2",
    "@switchboard-xyz/on-demand": "^1.2.14",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "mocha": "^10.3.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
